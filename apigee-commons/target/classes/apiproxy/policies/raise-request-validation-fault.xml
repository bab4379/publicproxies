<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="raise-request-validation-fault">
    <DisplayName>raise-request-validation-fault</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json" variablePrefix="@" variableSuffix="#">
                {
                    "message" : "Invalid Request for Grant Types"
                }
            </Payload>
            <StatusCode>400</StatusCode>
            <ReasonPhrase>Invalid Request for Grant Types</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>