<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="smFailedAuthorizationAndAuthentication">
    <DisplayName>smFailedAuthorizationAndAuthentication</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json" variablePrefix="@" variableSuffix="#">
                {
                    "message" : "Unable to validate user"
                }
            </Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unable to validate user</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
</RaiseFault>